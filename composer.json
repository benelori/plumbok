{
  "name": "benelori/plumbok",
  "description": "Runtime Code Generator like Lombok for PHP",
  "license": "MIT",
  "authors": [
    {
      "name": "Micha≈Ç Brzuchalski",
      "email": "michal.brzuchalski@gmail.com",
      "homepage": "https://github.com/brzuchal/"
    },
    {
      "name": "Maciej Kruszewski",
      "email": "m.kruszewski@sunflux.pl"
    }
  ],
  "require": {
    "php": "^7.1",
    "doctrine/annotations": "1.*",
    "phpdocumentor/reflection-docblock": "^4.3 | ^5.0",
    "symfony/console": "^4.0",
    "nikic/php-parser": "4.*",
    "doctrine/inflector": "^1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1",
    "phpunit/php-code-coverage": "6.*",
    "mikey179/vfsStream": "^1.6",
    "symfony/var-dumper": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "Plumbok\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plumbok\\Test\\": "tests/classes/"
    }
  },
  "bin": [
    "./bin/plumbok"
  ],
  "config": {
    "bin-dir": "bin/"
  }
}
